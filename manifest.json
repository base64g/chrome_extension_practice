{
    "manifest_version": 2,
    "name": "sociality_filter",
    "version": "1.0",
    "background": {
	"scripts": ["background.js"],
	"persistent": false
    },
    "browser_action": {
	"default_icon": "icon.png",
	"default_title": "sociality_filter"
    },
    "content_scripts": [
	{
	    "matches": ["<all_urls>"],
	    "js": ["jquery-3.1.1.min.js", "content.js"]
	}
    ]
}
